  "resolutions": {
    "eslint-plugin-jsx-a11y": "6.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "semistandard"
    ],
    "plugins": [
      "only-warn"
    ]
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
